{
  "name": "analytics-apps",
  "version": "0.1.0",
  "private": true,
  "homepage": "/analytics-apps",
  "scripts": {
    "start": "cross-env PORT=80 PUBLIC_URL=analytics-apps react-app-rewired start",
    "build": "react-app-rewired build && mkdir build/analytics-apps && mv build/asset-manifest.json build/analytics-apps/asset-manifest.json && mv build/static build/analytics-apps/static",
    "start:ci": "serve --no-clipboard --single --listen 80 build",
    "postcss": "postcss src/postcss/*.css -d src/css",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "CI=true react-app-rewired test --collect-coverage --collectCoverageFrom='**/*.tsx' --env=jest-environment-jsdom-sixteen",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "pretest:e2e:ci": "yarn && yarn build",
    "test:e2e:ci": "yarn&&start-server-and-test start:ci http://localhost:80 cypress:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:80 cypress",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"*/**/*.{js,jsx,ts,tsx,css,json,graphql,yml}\"",
    "format": "yarn prettier --write"
  },
  "dependencies": {
    "@reach/descendants": "0.10.4",
    "@reach/utils": "0.10.5",
    "@types/yamljs": "0.2.31",
    "axios": "0.19.2",
    "clsx": "1.1.1",
    "cross-env": "7.0.2",
    "downshift": "6.0.5",
    "formik": "2.1.4",
    "history": "4.10.1",
    "i18next": "19.7.0",
    "i18next-browser-languagedetector": "4.2.0",
    "js-cookie": "2.2.1",
    "lodash": "4.17.15",
    "match-sorter": "4.1.0",
    "node-sass": "4.14.1",
    "opsramp-design-system": "0.1.91",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "11.5.0",
    "react-query": "2.26.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-svg": "11.0.25",
    "regraph": "file:regraph-2.2.0+evaluation-2020-11-27.tgz",
    "use-deep-compare-effect": "1.3.1",
    "yamljs": "0.3.0",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "@testing-library/user-event": "12.1.4",
    "@types/history": "4.7.6",
    "@types/jest": "26.0.0",
    "@types/js-cookie": "2.2.6",
    "@types/lodash": "4.14.157",
    "@types/match-sorter": "4.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "8.0.0",
    "@types/yup": "0.29.3",
    "cypress": "4.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "husky": "4.2.5",
    "i18next": "19.7.0",
    "is-ci-cli": "2.1.1",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "lint-staged": "10.2.9",
    "prettier": "2.0.5",
    "react-app-rewire-stylus-modules": "0.0.2",
    "react-app-rewired": "2.1.6",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.0",
    "stop-runaway-react-effects": "2.0.0",
    "typescript": "3.9.5",
    "webpack-cli": "3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-types && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|css|json|graphql|md|yml)": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
